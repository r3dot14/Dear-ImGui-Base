{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: cl.exe build active file",
            "command": "cl.exe",
            "args": [
                "/Zi",
                "/EHsc",
                "/nologo",
                "/Ycpch.h",
                "/Yupch.h",
                "/ID:/Projects/VS Code Projects/MaciUtilityTool/src/",
                "/Fe${workspaceFolder}/build/Debug/${fileBasenameNoExtension}.exe",
                "/Fo${workspaceFolder}/build/Debug/",
                "/Fd${workspaceFolder}/build/Debug/vc140.pdb",
                "${workspaceFolder}/src/*.cpp",
                "${workspaceFolder}/imgui/*.cpp",
                "${workspaceFolder}/imgui/backends/*.cpp"
                
                
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$msCompile"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}